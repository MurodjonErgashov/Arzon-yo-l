  <script>
    const slicerContainer = document.getElementsByClassName('news_container');
    const slicerItems = document.querySelectorAll('.news_container .box');
    const itemsPerPage = 4;
    let currentPage = 0;

    function showPage(page) {
        const start = page * itemsPerPage;
        const end = start + itemsPerPage;
        
        slicerItems.forEach((item, index) => {
            item.style.display = (index >= start && index < end) ? 'block' : 'none';
        });
    }

    function nextPage() {
        currentPage = Math.min(currentPage + 1, Math.ceil(slicerItems.length / itemsPerPage) - 1);
        showPage(currentPage);
    }

    function prevPage() {
        currentPage = Math.max(currentPage - 1, 0);
        showPage(currentPage);
    }

    showPage(currentPage);

</script>

